<p id="notice"><%= notice %></p>

<h1>Listing Language Levels</h1>

<table>
  <thead>
    <tr>
      <th>Language</th>
      <th>Level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @language_levels.each do |language_level| %>
      <tr>
        <td><%= language_level.language_id %></td>
        <td><%= language_level.level %></td>
        <td><%= link_to 'Show', language_level %></td>
        <td><%= link_to 'Edit', edit_language_level_path(language_level) %></td>
        <td><%= link_to 'Destroy', language_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Language level', new_language_level_path %>
